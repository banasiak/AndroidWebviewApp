<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <SwitchPreference
        android:key="allow_content_access"
        android:title="@string/pref_allow_content_access_title"
        android:summary="@string/pref_allow_content_access_summary"
        android:defaultValue="true"
        />
    <SwitchPreference
        android:key="allow_file_access"
        android:title="@string/pref_allow_file_access_title"
        android:summary="@string/pref_allow_file_access_summary"
        android:defaultValue="true"
        />
    <SwitchPreference
        android:key="block_network_image"
        android:title="@string/pref_block_network_image_title"
        android:summary="@string/pref_block_network_image_summary"
        android:defaultValue="false"
        />
    <SwitchPreference
        android:key="block_network_loads"
        android:title="@string/pref_block_network_loads_title"
        android:summary="@string/pref_block_network_loads_summary"
        android:defaultValue="false"
